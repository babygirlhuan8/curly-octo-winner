{
    "name": "curly-octo-winner",
    "version": "1.0.0",
    "description": "学习记录webpack + Vue",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack --mode development",
        "watch": "webpack -w",
        "start": "webpack-dev-server --open",
        "lint": "eslint src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/babygirlhuan8/curly-octo-winner.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/babygirlhuan8/curly-octo-winner/issues"
    },
    "homepage": "https://github.com/babygirlhuan8/curly-octo-winner#readme",
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "autoprefixer": "^9.5.1",
        "babel-loader": "^8.0.6",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-vue": "^5.2.2",
        "file-loader": "^3.0.1",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "mini-css-extract-plugin": "^0.6.0",
        "postcss-import": "^12.0.1",
        "pre-commit": "^1.2.2",
        "style-loader": "^0.23.1",
        "sugarss": "^2.0.0",
        "vue-loader": "^14.2.4",
        "vue-router": "^3.0.6",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.31.0",
        "webpack-cli": "^3.3.2",
        "webpack-dev-server": "^3.4.1"
    },
    "dependencies": {
        "html-webpack-plugin": "^3.2.0",
        "vue": "^2.6.10",
        "vuex": "^3.1.1"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": [
            "plugin:vue/essential"
        ],
        "globals": {
            "Atomics": "readonly",
            "SharedArrayBuffer": "readonly"
        },
        "parserOptions": {
            "ecmaVersion": 2018,
            "sourceType": "module"
        },
        "plugins": [
            "vue"
        ],
        "rules": {
            "indent": [
                "error",
                4
            ],
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "always"
            ],
            "no-console": [
                "warn",
                {
                    "allow": [
                        "warn",
                        "error"
                    ]
                }
            ]
        }
    },
    "pre-commit": [
        "lint"
    ]
}
